<%= form_for @user do |f| %>
    
    <%= f.label :name, "Name" %>
    <%= f.text_field :name %>
    <br />
    <%= f.label :account, "Account" %>
    <%= f.text_field :account %>
    <br />
    <%= f.label :password, "Password" %>
    <%= f.password_field :password %>
    
    <hr />
    <%= f.submit "Create" %>
<% end %>
